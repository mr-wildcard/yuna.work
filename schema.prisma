datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model testimonial {
  id                              String          @id @default(cuid())
  content                         String          @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  author                          String          @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  context                         String          @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  from_Collaboration_testimonials Collaboration[] @relation("Collaboration_testimonials")
}

model Collaboration {
  id               String        @id @default(cuid())
  testimonials     testimonial[] @relation("Collaboration_testimonials")
  intro_highlights String?       @default("{\"title\":\"\",\"items\":[]}")
}
